{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "https://localhost:3000/#/",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}/wwwroot"
        },        
        {
            "name": "Launch Edge",
            "type": "edge",
            "request": "launch",
            "url": "https://localhost:3000/#/",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}/wwwroot"
        },
        {
            "name": "Attach to AddIn",
            "type": "edge",
            "request": "attach",
            "port": 9222,
            "url": "https://localhost:3000/#/",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}/wwwroot",
            "diagnosticLogging": true
        }
    ]
}