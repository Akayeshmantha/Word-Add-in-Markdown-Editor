{
  "name": "markdown-editor-for-word",
  "description": "Author your markdown files and push your changes to GitHub directly from Word - your favourite document editor.",
  "repository": "",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack-dev-server --https --inline --progress --compress --port 3000 --cert certificates/localhost.crt --key certificates/localhost.key --open",
